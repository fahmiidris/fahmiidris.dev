{
    "name": "fahmiidris.dev",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "check:dead-ts": "ts-prune --error",
        "check:ts": "tsc",
        "check:lint": "eslint . --quiet --ignore-path .gitignore",
        "checker": "npm run check:dead-ts; npm run check:ts; npm run check:lint",
        "format:lint": "eslint . --fix --quiet --ignore-path .gitignore",
        "format:prettier": "prettier --config prettier.config.cjs . --ignore-path .gitignore --write --loglevel silent",
        "formatter": "npm run staged:lint; npm run staged:prettier",
        "staged:lint": "eslint --cache --fix",
        "staged:prettier": "prettier --write -u",
        "prepare": "husky install"
    },
    "dependencies": {
        "clsx": "^1.2.1",
        "next": "13.4.3",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@types/node": "20.2.1",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "autoprefixer": "^10.4.14",
        "eslint": "8.41.0",
        "eslint-config-next": "13.4.3",
        "eslint-config-prettier": "^8.8.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "postcss": "^8.4.23",
        "postcss-focus-visible": "^8.0.2",
        "postcss-import": "^15.1.0",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "tailwindcss": "^3.3.2",
        "ts-prune": "^0.10.3",
        "typescript": "5.0.4"
    }
}
